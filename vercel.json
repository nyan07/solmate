{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "fluid": true,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/(.*)\\.(.*)",
      "dest": "/$1.$2"
    },
    {
      "src": "/(.*)",
      "dest": "/",
      "status": 200
    }
  ],
  "functions": {
    "api/**": {
      "supportsCancellation": true
    }
  }
}